RewriteEngine On
# Pages
RewriteRule ^Login ./Pages/Login.php
RewriteRule ^Signup ./Pages/Signup.php
Rewriterule ^Post ./Pages/Post.php

#Transfer Data
RewriteRule ^SignUpForm ./Validate.php
RewriteRule ^UserForm ./Validate.php
RewriteRule ^Logout ./Validate.php?Logout=true
RewriteRule ^UploadPost ./Validate.php

#Assets
RewriteRule ^Styles/General ./Asset/Styles/General.css